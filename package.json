{
  "name": "@nestjs/terminus",
  "version": "6.5.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@terminus)",
  "repository": {
    "url": "https://github.com/nestjs/terminus"
  },
  "license": "MIT",
  "author": "Livio Brunner",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp",
    "build:all": "gulp build:all",
    "clean": "gulp clean",
    "doc": "./node_modules/.bin/compodoc --disableInternal --disableSourceCode --disablePrivate -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run test:ut",
    "test:e2e": "jest --config=e2e/jest-e2e.json --detectOpenHandles --forceExit",
    "test:ut": "jest --config=jest.json",
    "test:ut:cov": "npm run test:ut -- --coverage"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json ./package.json",
      "git add"
    ]
  },
  "dependencies": {
    "check-disk-space": "2.1.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.10",
    "@godaddy/terminus": "4.1.2",
    "@nestjs/common": "6.5.3",
    "@nestjs/core": "6.5.3",
    "@nestjs/testing": "6.5.3",
    "@types/jest": "24.0.15",
    "@types/mongoose": "5.5.9",
    "@types/node": "12.6.8",
    "fastify": "2.6.0",
    "fastify-formbody": "3.1.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "5.0.1",
    "husky": "3.0.1",
    "jest": "24.8.0",
    "lint-staged": "9.2.0",
    "mysql": "2.17.1",
    "portfinder": "1.0.21",
    "prettier": "1.18.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.2",
    "sort-package-json": "1.22.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "@godaddy/terminus": "4.1.2",
    "@nestjs/common": "6.5.3",
    "@nestjs/core": "6.5.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2"
  },
  "optionalDependencies": {
    "@grpc/proto-loader": "0.5.1",
    "@nestjs/microservices": "6.5.3",
    "@nestjs/mongoose": "6.1.2",
    "@nestjs/platform-express": "6.5.3",
    "@nestjs/platform-fastify": "6.5.3",
    "@nestjs/typeorm": "6.1.3",
    "amqp-connection-manager": "3.0.0",
    "amqplib": "0.5.3",
    "grpc": "1.22.2",
    "mongoose": "5.6.5",
    "mqtt": "3.0.0",
    "nats": "1.3.0",
    "redis": "2.8.0",
    "typeorm": "0.2.18"
  }
}
