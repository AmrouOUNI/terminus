{
  "name": "@nestjs/terminus",
  "version": "6.5.6",
  "description": "Nest - modern, fast, powerful node.js web framework (@terminus)",
  "repository": {
    "url": "https://github.com/nestjs/terminus"
  },
  "license": "MIT",
  "author": "Livio Brunner",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp",
    "build:all": "gulp build:all",
    "clean": "gulp clean",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run test:ut",
    "test:e2e": "jest --config=e2e/jest-e2e.json --detectOpenHandles --forceExit",
    "test:ut": "jest --config=jest.json",
    "test:ut:cov": "npm run test:ut -- --coverage"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json ./package.json",
      "git add"
    ]
  },
  "dependencies": {
    "check-disk-space": "2.1.0",
    "deprecate": "^1.1.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@godaddy/terminus": "4.3.1",
    "typeorm": "0.2.24",
    "@nestjs/mongoose": "6.4.0",
    "@nestjs/typeorm": "7.0.0",
    "@nestjs/microservices": "7.0.5",
    "@nestjs/common": "7.0.5",
    "@nestjs/core": "7.0.5",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/testing": "7.0.5",
    "@types/deprecate": "^1.1.0",
    "@types/jest": "25.1.4",
    "@types/mongoose": "5.7.7",
    "@types/node": "13.9.3",
    "fastify": "2.13.0",
    "fastify-formbody": "3.1.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "5.0.1",
    "husky": "4.2.3",
    "jest": "25.2.0",
    "lint-staged": "10.0.9",
    "mysql": "2.18.1",
    "portfinder": "1.0.25",
    "prettier": "2.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "sort-package-json": "1.40.0",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-node": "8.8.1",
    "tslint": "6.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.2",
    "@nestjs/core": "^7.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  }
}
