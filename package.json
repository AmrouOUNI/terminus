{
  "name": "@nestjs/terminus",
  "version": "6.0.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@terminus)",
  "repository": {
    "url": "https://github.com/nestjs/terminus"
  },
  "license": "MIT",
  "author": "Livio Brunner",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "doc": "./node_modules/.bin/compodoc --disableProtected --disableInternal --disablePrivate -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run test:e2e && npm run test:ut",
    "test:e2e": "jest --config=e2e/jest-e2e.json --detectOpenHandles --forceExit",
    "test:ut": "jest --config=jest.json",
    "test:ut:cov": "npm run test:ut -- --coverage"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json ./package.json",
      "git add"
    ]
  },
  "dependencies": {
    "check-disk-space": "^1.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.9",
    "@godaddy/terminus": "4.1.0",
    "@grpc/proto-loader": "0.4.0",
    "@nestjs/common": "6.0.5",
    "@nestjs/core": "6.0.5",
    "@nestjs/microservices": "6.0.5",
    "@nestjs/platform-express": "6.0.5",
    "@nestjs/platform-fastify": "6.0.5",
    "@nestjs/testing": "6.0.5",
    "@types/jest": "24.0.11",
    "@types/mongoose": "5.3.24",
    "@types/node": "11.13.0",
    "amqp-connection-manager": "2.3.1",
    "amqplib": "0.5.3",
    "fastify": "2.2.0",
    "fastify-formbody": "3.1.0",
    "grpc": "1.19.0",
    "husky": "1.3.1",
    "jest": "24.5.0",
    "lint-staged": "8.1.5",
    "mqtt": "2.18.8",
    "mysql": "2.16.0",
    "nats": "1.2.6",
    "opn": "5.5.0",
    "portfinder": "1.0.20",
    "prettier": "1.16.4",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.4.0",
    "rxjs-compat": "6.4.0",
    "sort-package-json": "1.22.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.2"
  },
  "peerDependencies": {
    "@godaddy/terminus": "^4.1.0",
    "@grpc/proto-loader": "^0.4.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/mongoose": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-fastify": "^6.0.0",
    "@nestjs/typeorm": "^6.0.0",
    "amqp-connection-manager": "^2.3.0",
    "amqplib": "^0.5.3",
    "grpc": "^1.19.0",
    "mongoose": "^5.4.19",
    "nats": "^1.2.2",
    "redis": "^2.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^6.4.0",
    "typeorm": "^0.2.14"
  },
  "optionalDependencies": {
    "@grpc/proto-loader": "^0.4.0",
    "@nestjs/microservices": "^6.0.0",
    "@nestjs/mongoose": "^6.0.0",
    "@nestjs/typeorm": "^6.0.0",
    "amqp-connection-manager": "^2.3.0",
    "amqplib": "^0.5.3",
    "grpc": "^1.19.0",
    "mongoose": "^5.4.19",
    "mqtt": "^2.18.8",
    "nats": "^1.2.2",
    "redis": "^2.8.0",
    "typeorm": "^0.2.14"
  }
}
